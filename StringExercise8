package B3;

public class StringExerciseQ8 {
	    public static boolean isNumeric(String input) {
	        return input != null && input.matches("\\d+");
	    }

	    public static void main(String[] args) {
	        String test1 = "12345";
	        String test2 = "123A5";

	        System.out.println("Is numeric (12345)? " + isNumeric(test1)); // true
	        System.out.println("Is numeric (123A5)? " + isNumeric(test2)); // false
	    }
	}



